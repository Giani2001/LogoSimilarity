			<script type="text/javascript">
				var tags = [];
				var grid = $('.magazineContainer').isotope({
						  itemSelector: '.mag-item',
						  percentPosition: true,
						  masonry: {
						  	columnWidth: '.mag-sizer',
						  	gutter: '.mag-gutter'
						  }
						});
				/* Category Filter */
				$('.magNavList').on( 'click', 'div', function() {
  					var filterValue = $(this).attr('data-filter');
  					$(this).parent().find(".selected").removeClass("selected");
  					$(this).addClass("selected");
  					grid.isotope({ filter: filterValue });
				});
				/* Tags Filter */
				/* add Tags */				
				var buttons = "";
				$('.tags .filters').on( 'click', 'span', function() {
					$(this).toggleClass("selected");
					if($(this).hasClass("selected")) {
						tags.push($(this).attr('data-filter'));
					} else {
						var pos = tags.indexOf($(this).attr('data-filter'));
						tags.splice(pos, 1);
					}	
  					var filterValue = tags.toString();
  					$('.filterThemeContainer').removeClass("hide");
  					buttons = "";
  					$.each(tags, function(index,value) {
  						buttons += "<span class=\"filter\" data-set=\""+value.substr(1,value.length)+"\">"+value.substr(1,value.length)+"<i class=\"icon-interface-box-cross text-primary ml-2\"></i></span>";
  					})
  					$('#filterThemes').empty();
  					$('#filterThemes').append(buttons);
  					grid.isotope({ filter: filterValue });
				});
				/* remove Tags */
				$('#filterThemes').on( 'click', 'span', function() {
					var filterdata = "."+$(this).attr('data-set');
					var pos = tags.indexOf(filterdata);
					tags.splice(pos, 1);
					if(pos > 0) {
						var filterValue = tags.toString();
						buttons = "";
	  					$.each(tags, function(index,value) {
  							buttons += "<span class=\"filter\" data-set=\""+value.substr(1,value.length)+"\">"+value.substr(1,value.length)+"<i class=\"icon-interface-box-cross text-primary ml-2\"></i></span>";
  						})
  						$('#filterThemes').empty();
  						$('#filterThemes').append(buttons);
					} else{
  						$('.filterThemeContainer').addClass("hide");
  					}
  					grid.isotope({ filter: filterValue });
  					$('.tags .filters .filter').each(function( index ) {
  						if($(this).attr('data-filter') === filterdata) {
  							$(this).removeClass('selected');
  						}
  					}); 
				});
				/* load more */		
				var initShow = 50; //number of items loaded on init & onclick load more button
				var counter = initShow; //counter for load more button
				var iso = grid.data('isotope'); // get Isotope instance
				function loadMore(toShow) {
					grid.find(".hidden").removeClass("hidden");
					var hiddenElems = iso.filteredItems.slice(toShow, iso.filteredItems.length).map(function(item) {
						return item.element;
					});
					$(hiddenElems).addClass('hidden');
						grid.isotope('layout');
					if (hiddenElems.length == 0) {
						jQuery("#btn-more").hide();
					} else {
						jQuery("#btn-more").show();
					};
				}
				grid.after('<div class="container-fluid mb-5"><div class="row"><div class="col-12 col-md-6 col-lg-4 offset-md-6 offset-lg-8"><span class="btn btn-block btn-primary append-button" id="btn-more" style="text-align:center;">Vis flere</span></div></div></div>');
				$("#btn-more").click(function() {
					if ($('.filters').data('clicked')) {
						counter = initShow;
					    $('.filters').data('clicked', false);
					} else {
						counter = counter;
					};
					counter = counter + initShow;
					loadMore(counter);
				});		
				$(window).on("load",function() {
					loadMore(initShow); //execute function onload
					equalHeight($(".mag-item .textcontainer"));
					loadMore(initShow); //execute function onload
				});
			</script>
